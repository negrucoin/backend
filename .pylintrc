[MASTER]
ignore=tests,
       migrations
init-hook='import sys; sys.path.insert(0, "/negrucoin")'

[MESSAGES CONTROL]
disable=raw-checker-failed,
        bad-inline-option,
        locally-disabled,
        file-ignored,
        suppressed-message,
        useless-suppression,
        deprecated-pragma,
        use-symbolic-message-instead,
        missing-module-docstring,
        import-error,
        no-name-in-module